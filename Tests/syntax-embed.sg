# FEATURE: Embedding code
# SYNTAX: @embed Language ... @end

@class A
	@embed JavaScript
	|var hello = {world:"Hello, World !"};
	|alert("HELLO WORLD" + hello.world)
	@end
@end

@function hello
	@embed JavaScript
	|var hello = {world:"Hello, World !"};
	|alert("HELLO WORLD" + hello.world)
	@end
@end
